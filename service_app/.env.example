# OpenRouter API Key (required for DeepConf strategy)
# Get your key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your-openrouter-api-key-here

# OpenAI API Key (optional, for direct OpenAI API access)
OPENAI_API_KEY=your-openai-api-key-here

# Server Settings (optional)
PORT=8001
HOST=0.0.0.0

# vLLM Backend (for offline_bon, online_bon, beam_search strategies)
VLLM_MODEL_PATH=Qwen/Qwen2.5-Coder-7B-Instruct
VLLM_MAX_MODEL_LEN=32000
# Reduce to fit PRM model on same GPU: 0.5 for main + 0.3 for PRM = 0.8 total
VLLM_GPU_MEMORY_UTILIZATION=0.5

# PRM Scorer (optional, for tts_scorer=prm)
PRM_MODEL_PATH=Qwen/Qwen2.5-Math-PRM-7B
PRM_DEVICE=cuda:0
PRM_BATCH_SIZE=8
PRM_TORCH_DTYPE=bfloat16
PRM_USE_VLLM=true
PRM_GPU_MEMORY_UTILIZATION=0.3
